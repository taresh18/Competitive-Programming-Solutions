#include<bits/stdc++.h>

using namespace std;

int main(){
	int r, c;
	cin>>r>>c;
	if(r==1 && c==1) cout<<"0";
	else{
		if(r<=c){
	    	for(int i=1; i<=r; i++){
		    	for(int j=r+1; j<=r+c; j++) cout<<i*j<<" ";
		    	cout<<endl;
	        }
	    }
	    else{
	    	for(int i=c+1; i<=r+c; i++){
		    	for(int j=1; j<=c; j++) cout<<i*j<<" ";
		    	cout<<endl;
	        }
	    }
	}
}