#include<bits/stdc++.h>
using namespace std;

typedef long long ll;
const int MAX=1000003;
ll larPrime[MAX];

void faster(){
	ios_base::sync_with_stdio(false);
	cin.tie(NULL);
	cout.tie(NULL);
}

int main(){
    int a,n,i,j;
    cin>>n;
    a = (n*n)/2;
    for(i=1, j=(n*n); i<=a && j>a; i++, j--){
        cout<<i<<" "<<j<<endl;
    }
}