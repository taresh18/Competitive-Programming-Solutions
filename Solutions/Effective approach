#include<bits/stdc++.h>

using namespace std;
int a[100010];
int b[100010];
typedef long long ll;

int main()
{
    int n,m,c,x,d;
    ll j1=0,j2=0;
    cin>>n;
    for(int i=0;i<n;i++)
    {
       cin>>c;
       a[c]=i;
    }
    cin>>m;
    for(int i=0;i<m;i++)
    {
        cin>>x;
        int d=a[x];
        j1+=d+1;
        j2+=n-d;
 
    }
    cout<<j1<<" "<<j2<<endl;
    return 0;
}
