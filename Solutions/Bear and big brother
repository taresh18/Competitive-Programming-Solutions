#include <bits/stdc++.h>
 
using namespace std;
 
int main() {
    long double a, b, age;
    
    cin >> a >> b;
    
    long double log1 = log10(b / a);
    long double log2 = log10(1.5);
    
    if (a != b) {
        cout << ceil(log1 / log2);
    } else {
        cout << 1;
    }
    
    return 0;
}